      <li>DamageEvent_Include</li>
    </include>
    <rulePack>
      <rulesStrings>
       <li>damage_source->[cryptofreeze] froze</li>

            <li>cryptofreeze(p=3)->cryptofreeze</li>
            <li>cryptofreeze(p=3)->a cryptofreeze</li>
            <li>cryptofreeze(p=3)->the cryptofreeze</li>
            <li>cryptofreeze->cold snap</li>
            <li>cryptofreeze->a chill</li>
            <li>cryptofreeze->a froststorm</li>

            <li>damaged_present->freezing</li>
            <li>damaged_present->numbing</li>
            <li>damaged_present->biting cold</li>
            <li>damaged_present->frosting over</li>

            <li>destroyed_present->shattering</li>
            <li>destroyed_present->crystallizing</li>
            <li>destroyed_present->frostburning</li>
            <li>destroyed_present->encasing in ice</li>
