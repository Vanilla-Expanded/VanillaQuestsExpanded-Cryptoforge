<Defs>
	<ThingDef ParentName="VQE_CryptoForgeBuildingBase">
		<label>twisted metal</label>
		<defName>VQE_TwistedMetal</defName>
		<thingClass>VanillaQuestsExpandedCryptoforge.Lootable</thingClass>
		<description>A mangled piece of metal, twisted from the sheer force of the cryptoforge spaceship crash. Some steel can still be salvaged.</description>
		<clearBuildingArea>true</clearBuildingArea>
		<drawerType>MapMeshAndRealTime</drawerType>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.3</fillPercent>
		<graphicData>
			<texPath>Things/Junk/TwistedMetalSmall</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<uiIconScale>0.75</uiIconScale>
		<rotatable>true</rotatable>
		<pathCost>102</pathCost>
		<size>(1, 1)</size>
		<statBases>
			<Flammability>0</Flammability>
			<MaxHitPoints>100</MaxHitPoints>
		</statBases>
		<costList>
			<ChunkSlagSteel>2</ChunkSlagSteel>
		</costList>
		<modExtensions>
			<li Class="VanillaQuestsExpandedCryptoforge.LootableContents">
				<contents>
					<li>
						<thing>Steel</thing>
						<count>15</count>
					</li>
				</contents>
				<deconstructSound>Building_Deconstructed</deconstructSound>
			</li>
		</modExtensions>
		<building>
			<paintable>false</paintable>
			<claimable>false</claimable>
			<neverBuildable>true</neverBuildable>
		</building>
	</ThingDef>
